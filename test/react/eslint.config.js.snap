        "escape": "readonly",
        "eval": "readonly",
        "exports": "writable",
        "fetch": "readonly",
        "global": "readonly",
        "globalThis": "readonly",
        "isFinite": "readonly",
        "isNaN": "readonly",
        "module": "readonly",
        "navigator": "readonly",
        "parseFloat": "readonly",
        "parseInt": "readonly",
        "performance": "readonly",
        "process": "readonly",
        "queueMicrotask": "readonly",
        "require": "readonly",
        "setImmediate": "readonly",
        "setInterval": "readonly",
        "setTimeout": "readonly",
        "structuredClone": "readonly",
        "undefined": "readonly",
