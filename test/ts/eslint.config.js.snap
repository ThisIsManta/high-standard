            ],
            "markers": [
              "*package",
              "!",
              ",",
              ":",
              "::",
              "flow-include",
            ],
          },
          "line": {
            "markers": [
              "*package",
              "!",
              "/",
              ",",
              "=",
            ],
          },
        },
      ],
      "@stylistic/switch-colon-spacing": [
        "error",
        {
          "after": true,
          "before": false,
        },
      ],
      "@stylistic/template-curly-spacing": [
        "error",
        "never",
      ],
      "@stylistic/template-tag-spacing": [
        "error",
        "never",
      ],
      "@stylistic/wrap-iife": [
        "error",
        "any",
        {
          "functionPrototypeMethods": true,
        },
      ],
      "@stylistic/yield-star-spacing": [
        "error",
        "after",
      ],
      "camelcase": "warn",
      "consistent-return": "error",
      "constructor-super": "error",
      "curly": [
        "error",
        "all",
      ],
      "dot-notation": [
        "error",
        {
          "allowKeywords": true,
        },
      ],
      "eqeqeq": [
        "error",
        "always",
        {
          "null": "ignore",
        },
      ],
      "import/export": "error",
      "import/first": "error",
      "import/named": "error",
      "import/no-absolute-path": [
        "error",
