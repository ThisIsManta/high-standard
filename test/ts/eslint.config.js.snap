  },
  {
    "files": [
      "**/*.{ts,mts,cts,js,mjs,cjs}",
    ],
    "ignores": [
      "../../node_modules/**/*",
    ],
    "languageOptions": {
      "parser": true,
      "parserOptions": {
        "ecmaVersion": undefined,
        "jsx": false,
        "project": "tsconfig.json",
        "tsconfigRootDir": "ts",
      },
    },
    "name": "typescript:tsconfig.json",
    "plugins": {
      "@typescript-eslint": true,
    },
    "rules": {
      "@stylistic/member-delimiter-style": [
        "error",
        {
          "multiline": {
            "delimiter": "none",
            "requireLast": false,
          },
          "singleline": {
            "delimiter": "comma",
            "requireLast": false,
          },
        },
      ],
      "@stylistic/type-annotation-spacing": [
        "error",
        {
          "after": true,
          "before": true,
          "overrides": {
            "arrow": {
              "after": true,
              "before": true,
            },
            "colon": {
              "after": true,
              "before": false,
            },
          },
        },
      ],
      "@stylistic/type-generic-spacing": "error",
      "@stylistic/type-named-tuple-spacing": "error",
      "@typescript-eslint/adjacent-overload-signatures": "error",
      "@typescript-eslint/array-type": [
        "error",
        {
          "default": "generic",
          "readonly": "generic",
        },
      ],
      "@typescript-eslint/consistent-type-assertions": "error",
      "@typescript-eslint/naming-convention": [
        "warn",
        {
          "format": [
            "camelCase",
            "PascalCase",
            "UPPER_CASE",
          ],
          "selector": "variable",
        },
        {
          "format": [
            "camelCase",
            "PascalCase",
          ],
          "selector": "function",
        },
        {
          "format": null,
          "modifiers": [
            "destructured",
          ],
          "selector": [
            "variable",
            "parameter",
          ],
        },
        {
          "format": [
            "camelCase",
          ],
          "leadingUnderscore": "allow",
          "selector": "parameter",
        },
        {
          "format": [
            "PascalCase",
          ],
          "selector": [
            "typeLike",
            "enumMember",
          ],
        },
        {
          "format": [
            "PascalCase",
          ],
          "modifiers": [
            "exported",
          ],
          "prefix": [
            "I",
          ],
          "selector": "interface",
        },
        {
          "format": [
            "camelCase",
          ],
          "leadingUnderscore": "allowSingleOrDouble",
          "modifiers": [
            "private",
          ],
          "selector": "memberLike",
        },
        {
          "format": [
            "camelCase",
            "snake_case",
          ],
          "leadingUnderscore": "allowSingleOrDouble",
          "selector": "memberLike",
        },
        {
          "format": null,
          "modifiers": [
            "requiresQuotes",
          ],
          "selector": [
            "memberLike",
            "objectLiteralProperty",
          ],
        },
        {
          "format": [
            "camelCase",
            "snake_case",
          ],
          "leadingUnderscore": "allowSingleOrDouble",
          "selector": "objectLiteralProperty",
          "trailingUnderscore": "allowSingleOrDouble",
        },
      ],
      "@typescript-eslint/no-namespace": "error",
      "@typescript-eslint/no-this-alias": "error",
      "@typescript-eslint/no-use-before-define": [
        "error",
        {
          "classes": false,
          "functions": false,
          "variables": false,
        },
      ],
      "@typescript-eslint/prefer-for-of": "error",
      "@typescript-eslint/triple-slash-reference": [
        "error",
        {
          "types": "prefer-import",
        },
      ],
      "camelcase": "off",
      "import/export": "off",
      "import/named": "off",
      "levitate/no-top-level-require": "error",
      "levitate/typescript-explicit-return-type": [
        "error",
        "onlyIfMoreThanOneReturns",
      ],
      "levitate/typescript-method-type": "error",
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-use-before-define": "off",
    },
  },
]